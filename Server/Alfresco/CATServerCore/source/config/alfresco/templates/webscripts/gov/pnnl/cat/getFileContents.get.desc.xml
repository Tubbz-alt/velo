<webscript>

  <shortname>Get File Contents</shortname>
  <description>Get the file using a path or uuid and a content property</description>
  <url>/cat/getFileContents?path={?path}&amp;uuid={?uuid}&amp;property={?property}</url>
  <authentication>user</authentication>
  <!--  we don't require default tx because we manage our own retrying tx -->
  <transaction>none</transaction>
    
</webscript>