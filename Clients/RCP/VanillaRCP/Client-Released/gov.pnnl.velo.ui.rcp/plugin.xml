<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="gov.pnnl.velo.ui.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="gov.pnnl.velo.ui.rcp.application"
            name="Velo">
         <property
               name="appName"
               value="Velo">
         </property>
         <property
               name="windowImages"
               value="icons/velo-16x16.png,icons/velo-32x32.png,icons/velo-48x48.png">
         </property>
		  <property
				name="applicationXMI"
				value="gov.pnnl.velo.ui.rcp/LegacyIDE.e4xmi">
		  </property>
		  <property
				name="cssTheme"
				value="org.eclipse.e4.ui.css.theme.high-contrast">
		  </property>
		  <property
				name="applicationCSSResources"
          value="platform:/plugin/org.eclipse.ui.themes/images/">
		  </property>

          <!--         
         <property
               name="applicationCSS"
               value="platform:/plugin/org.eclipse.ui.themes/css/e4_default_win7.css">
          </property> 
          -->  
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="gov.pnnl.velo.ui.rcp.InteractiveSplashHandler"
            id="gov.pnnl.velo.ui.rcp.splashHandler">
      </splashHandler>
      <splashHandlerProductBinding
            productId="gov.pnnl.velo.ui.rcp.product"
            splashId="gov.pnnl.velo.ui.rcp.splashHandler">
      </splashHandlerProductBinding>
   </extension>

</plugin>
