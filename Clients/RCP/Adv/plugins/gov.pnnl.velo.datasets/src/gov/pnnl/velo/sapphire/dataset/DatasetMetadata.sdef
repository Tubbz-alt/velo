                <hint>
                  <name>assist.contributors</name>
                  <value>HelpActionContributor</value>
                </hint>
              </child-property>
              <hint>
                  <name>show.header</name>
                  <value>false</value>
              </hint>
              <height>40</height>
              <hint>
                <name>factory</name>
                <value>CompactListPropertyEditorPresentation$Factory</value>
              </hint>
            </property-editor>
            <spacer>
