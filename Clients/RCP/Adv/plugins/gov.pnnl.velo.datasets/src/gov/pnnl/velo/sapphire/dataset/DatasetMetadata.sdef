                </action-handler-id>
                <label>Add Image/Video Gallery Viewer</label>
              </actuator>
            </content>
          </section>

          <node-factory>
            <property>Viewers</property>
            <case>
              <label>Image And Video Gallery Viewer</label>
              <label>Image Gallery Viewer</label>
              <section>
                <content>
                  <property-editor>
                    <property>Type</property>
                    <hint>
                      <name>read.only</name>
                      <value>true</value>
                    </hint>
                    <hint>
                      <name>suppress.assist.contributors</name>
                      <value>System.ShowInSourceActionContributor</value>
