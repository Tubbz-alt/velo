              </child-property>
              <hint>
                <name>suppress.assist.contributors</name>
                <value>System.ShowInSourceActionContributor</value>
              </hint>
              <hint>
                <name>assist.contributors</name>
                <value>HelpActionContributor</value>
              </hint>
              <hint>
                  <name>show.header</name>
                  <value>false</value>
              </hint>
              <height>100</height>
            </property-editor>
            <spacer>
                <size>5</size>
            </spacer>
            <property-editor>
              <property>SponsorOrgs</property>
              <child-property>
                <property>SponsorOrg</property>
              </child-property>
              <hint>
                <name>suppress.assist.contributors</name>
                <value>System.ShowInSourceActionContributor</value>
              </hint>
              <hint>
                <name>assist.contributors</name>
                <value>HelpActionContributor</value>
              </hint>
              <hint>
                <name>show.header</name>
                <value>false</value>
              </hint>
              <height>100</height>
              <scale-vertically>true</scale-vertically>
            </property-editor>
