<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
This is the shortcut specification for windows XP. Its considered default so on
Systems that are not shortcut compatible the information is taken from here.
-->
<shortcuts>
<programGroup defaultName="${PRODUCT_DISPLAY_NAME}" location="startMenu"/>
<shortcut
    name = "${PRODUCT_DISPLAY_NAME}"
    target = "$INSTALL_PATH\${PRODUCT_NAME}.exe"
    commandLine = ""
    workingDirectory= "$INSTALL_PATH"
    description="Launch ${PRODUCT_DISPLAY_NAME}"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="no"/>
    
<shortcut
    name = "Uninstall"
    target = "$INSTALL_PATH\jre\bin\javaw.exe"
    commandLine = "-jar
    &quot;$INSTALL_PATH\Uninstaller\uninstall.jar&quot;"
    workingDirectory= "$INSTALL_PATH"
    description="Uninstall ${PRODUCT_DISPLAY_NAME}"
    iconIndex="1"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="no"/>
</shortcuts>