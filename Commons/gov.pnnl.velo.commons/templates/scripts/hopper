#!/bin/sh
# akunatool  job script generated by akunaclient
# based on the template hopper

#PBS -q ##queuename##
#PBS -l mppwidth=##procs##
#PBS -A ##account##
#PBS -l walltime=##walltime##
#PBS -j eo
#PBS -e akunajob.err
#PBS -o akunajob.out

cd $PBS_O_WORKDIR

# Run dos2unix on all text files
for f in *
do
  if [ `file $f |grep -c text` != "0" ]; then
     dos2unix $f 2> /dev/null
  fi
done

rm -f .status
umask 0
echo Starting `date`
#run agni in serial so that we can run amanzi using syscall and in parallel
##code## >& akunajob.out ;
echo $? > .status
echo Ending `date`